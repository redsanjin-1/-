.container{
  position: fixed;
  top:0;
  bottom:0;
  left:0;
  right:0;
  background-color: #b3d4db;
}
.profile{
  display: block;
  width:200rpx;
  height:200rpx;
  margin: 100rpx auto 0 auto
}
.title{
  display: block;
  width:100%;
  margin-top: 100rpx;
  text-align: center;
  font-weight: bold;
  font-size: 16px
}
.btn-start-wrapper{
  width:30%;
  margin:200rpx auto 0 auto;
  border:1px solid #405f80;
  border-radius: 4px;
  height:80rpx;
  line-height: 80rpx;
  text-align: center  
}
.btn-start{
  color:#405f80;
  font-size: 14px;
}